{
  "name": "libretier",
  "version": "1.0.0",
  "description": "LibreTier - A free, open-source tier list maker. Create, customize, and share tier-based rankings with drag-and-drop simplicity.",
  "keywords": [
    "libretier",
    "tier-list",
    "tier-maker",
    "ranking",
    "open-source",
    "drag-and-drop",
    "react",
    "nextjs",
    "typescript"
  ],
  "author": "ahmed-abdat (https://github.com/ahmed-abdat)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmed-abdat/libretier.git"
  },
  "bugs": {
    "url": "https://github.com/ahmed-abdat/libretier/issues"
  },
  "homepage": "https://github.com/ahmed-abdat/libretier#readme",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --cache-location .next/cache/eslint/ src",
    "lint:fix": "eslint --cache --cache-location .next/cache/eslint/ src --fix",
    "lint:strict": "eslint src --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "prepare": "husky"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@serwist/next": "^9.4.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.25",
    "html2canvas-pro": "^1.6.2",
    "lucide-react": "^0.556.0",
    "lz-string": "^1.5.0",
    "next": "16.0.10",
    "next-themes": "^0.4.6",
    "pako": "^2.1.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.1",
    "react-day-picker": "^9.12.0",
    "react-dom": "19.2.1",
    "react-dropzone": "^14.3.8",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "vaul": "^1.1.2",
    "zundo": "^2.3.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "16.0.7",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/lz-string": "^1.5.0",
    "@types/node": "^24.10.1",
    "@types/pako": "^2.0.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "serwist": "^9.4.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  },
  "lint-staged": {
    "*.{ts,tsx,json,md,css}": [
      "prettier --write"
    ]
  }
}
